			 </topic>
			 <topic>
				<name>WeatherInfo</name>
				 <stream.number>4</stream.number>
			 </topic>
		</topic.list>
		<group.id>group001</group.id>
		<serializer.class>kafka.serializer.StringEncoder</serializer.class>
		</consumer>
